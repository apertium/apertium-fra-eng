<rules>
  <rule><match lemma="parent" tags="n.pl"><select lemma="parent" tags="n.*"/></match></rule>
  <rule><match lemma="house" tags="n.*"><select lemma="maison" tags="n.*"/></match></rule>
  <rule><match lemma="go" tags="vblex.*"><select lemma="aller" tags="vblex.*"/></match></rule>
</rules>

